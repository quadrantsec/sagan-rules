#alert any $HOME_NET any -> $HOME_NET any (msg:"[AWS-CLOUDTRAIL] AWS API Discovery Commands"; program:s3.amazonaws.com; json_meta_content:".eventName",ListBuckets,ListObjects,GetBucketLocation,GetObjects,DeleteObjects; after:track by_username, count 5, seconds 60; threshold:type suppress, track by_username, count 1, seconds 28800; parse_src_ip:1; json_map:"username",".userIdentity.sessionContext.sessionIssuer.principalId"; reference:url,https://d1.awsstatic.com/events/aws-reinforce-2022/TDR431_The-anatomy-of-a-ransomware-event-targeting-data-residing-in-Amazon-S3.pdf; classtype:trojan-activity; sid:5014179; rev:1; metadata:deployment Endpoint,affected_product NONE,affected_version NONE,mitigation NONE,deprecation_reason NONE,tag Threat_Hunting, created_at 2023_01_20, updated_at 2023_01_20, mitre_tactic_id TA0007, mitre_technique_id T1082;)
