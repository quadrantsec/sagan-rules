alert any $HOME_NET any -> $EXTERNAL_NET any (msg:"[CROWDSTRIKE] CrowdStrike High Severity Event Reported "; content: "DetectionSummaryEvent"; nocase; content: "SeverityName=|22|High"; nocase; classtype:exploit-attempt; sid:5004756; rev:2;)

