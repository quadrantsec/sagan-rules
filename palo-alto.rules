########URLZ

alert any $HOME_NET any -> $EXTERNAL_NET any (msg: "[PALO-ALTO] Malware URL Identified"; content:",THREAT,url,"; content:!",online-personal-storage,"; content:!",block-url,"; content:",malware,"; threshold: type suppress, count 1, seconds 3600, track by_src; normalize; parse_port; parse_proto; parse_src_ip: 1; parse_dst_ip: 2; default_proto: tcp; default_dst_port: $HTTP_PORT; content: ",vsys"; classtype: suspicious-traffic; reference: url,www.brightcloud.com/tools/url-ip-lookup.php; sid:5002749; rev:7;)

alert any $HOME_NET any -> $EXTERNAL_NET any (msg: "[PALO-ALTO] Phishing URL Blocked"; content:",THREAT,url,"; content:",block-url,"; content:!",online-personal-storage,"; content:",phishing-and-other-frauds,"; threshold: type suppress, count 1, seconds 600, track by_src; normalize; parse_port; parse_proto; parse_src_ip: 1; parse_dst_ip: 2; default_proto: tcp; default_dst_port: $HTTP_PORT; content: ",vsys"; classtype: suspicious-traffic; reference: url,www.brightcloud.com/tools/url-ip-lookup.php; sid: 5002750; rev:4;)

alert any $HOME_NET any -> $EXTERNAL_NET any (msg: "[PALO-ALTO] Spyware or Adware URL Blocked"; content:",THREAT,url,"; content:",block-url,"; content:!",online-personal-storage,"; content:",spyware-and-adware,"; threshold: type suppress, count 1, seconds 1800, track by_src; normalize; parse_port; parse_proto; parse_src_ip: 1; parse_dst_ip: 2; default_proto: tcp; default_dst_port: $HTTP_PORT; content: ",vsys"; classtype: suspicious-traffic; reference: url,www.brightcloud.com/tools/url-ip-lookup.php; sid: 5002751; rev:4;)

alert any $HOME_NET any -> $EXTERNAL_NET any (msg: "[PALO-ALTO] Potential Bad URL Identified"; content:",THREAT,url,"; content:!",informational,"; nocase; content:!",command-and-control,"; nocase; content:!",online-personal-storage,"; content:!",malware-sites,"; content:!",phishing-and-other-frauds,"; content:!",spyware-and-adware,"; threshold: type suppress, count 1, seconds 1800, track by_dst; normalize; parse_port; parse_proto; parse_src_ip: 1; parse_dst_ip: 2; default_proto: tcp; default_dst_port: $HTTP_PORT; content: ",vsys"; classtype: suspicious-traffic; reference: url,https://docs.paloaltonetworks.com/pan-os/10-1/pan-os-admin/monitoring/use-syslog-for-monitoring/syslog-field-descriptions/url-filtering-log-fields; sid:5002752; rev:3;)

#####
##### Following rule is used in conjunction with meta_content variable IGNOREDL and set silent xbit which are checked in rule 5002762

#alert any $HOME_NET any -> $EXTERNAL_NET any (msg: "[PALO-ALTO] Url silent xbit set"; content:",THREAT,url,"; content:!",block-url,"; meta_content: "%sagan%",$IGNOREDL; meta_nocase; xbits: set,downloadnolog,track ip_src, expire 60; xbits:nounified2; xbits:noeve; normalize; parse_port; parse_proto; parse_src_ip: 1; parse_dst_ip: 2; default_proto: tcp; content: ",vsys"; classtype: suspicious-traffic; reference: url,www.brightcloud.com/tools/url-ip-lookup.php; sid: 5002754; rev:6;)

#####VIRI

alert any $HOME_NET any -> $EXTERNAL_NET any (msg: "[PALO-ALTO] Virus Detected"; content:"THREAT,virus"; normalize; parse_proto; parse_port; parse_src_ip: 1; parse_dst_ip: 2; default_proto: tcp; content: ",vsys"; classtype: suspicious-traffic; reference: url,threatvault.paloaltonetworks.com; sid: 5002755; rev:2;)

