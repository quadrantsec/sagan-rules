######Spyware DNS

alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[PALO-ALTO] Suspicious DNS Request"; content:"THREAT,"; content:",Suspicious DNS Query"; meta_content:"|2c|%sagan%|2c|",alert,continue,allow; normalize; parse_proto; parse_port; parse_src_ip: 1; parse_dst_ip: 2; default_proto: udp; default_dst_port: $DNS_PORT; content: ",vsys"; classtype: network-event; threshold: type suppress, track by_src, count 1, seconds 14400; reference: url,threatvault.paloaltonetworks.com; sid:5002763; rev:6;)

