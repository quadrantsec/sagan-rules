# Windows Bluedot rules.   
# Eventlog to syslog service.  This is what we primarily use.
# http://code.google.com/p/eventlog-to-syslog/

alert any $HOME_NET any -> $EXTERNAL_NET any (msg: "[WINDOWS-BLUEDOT] RDP / Logon type 10 from a Bluedot listed IP"; program: *Security*; event_id: 4624,540,528; content: "Logon Type|3a| 10 "; bluedot: type ip_reputation, track by_src, mdate_effective_period 3 months, Malicious,Tor,Honeypot,Proxy; program: *Security*; parse_src_ip: 1; normalize; default_proto: tcp; metadata: endpoint_command FIREWALL_BLOCK_SRC_IP; classtype: successful-user; sid:5002344; rev:10;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-BLUEDOT] Login failure from a Bluedot listed IP [0/5]"; program: *Security*; event_id: 529; metadata: endpoint_command FIREWALL_BLOCK_SRC_IP; classtype: unsuccessful-user; bluedot: type ip_reputation, track by_src, mdate_effective_period 3 months, Malicious,Tor,Honeypot,Proxy; threshold: type suppress, track by_src, count 5, seconds 300; normalize; parse_src_ip: 1; parse_port; sid:5002345; rev:9;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-BLUEDOT] Login failure from a Bluedot listed IP [Time restriction] [0/5]"; event_id: 530; metadata: endpoint_command FIREWALL_BLOCK_SRC_IP; classtype: unsuccessful-user; program: *Security*; parse_src_ip: 1; bluedot: type ip_reputation, track by_src, mdate_effective_period 3 months, Malicious,Tor,Honeypot,Proxy; threshold: type suppress, track by_src, count 5, seconds 300; normalize; sid:5002346; rev:9;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-BLUEDOT] Login failure from Bluedot listed IP - Account currently disabled [0/5]"; event_id: 531; content:!"User Name|3a| Domain|3a|"; metadata: endpoint_command FIREWALL_BLOCK_SRC_IP; classtype: unsuccessful-user; program: *Security*; bluedot: type ip_reputation, track by_src, mdate_effective_period 3 months, Malicious,Tor,Honeypot,Proxy; normalize; parse_src_ip: 1; threshold: type suppress, track by_src, count 5, seconds 300; sid:5002347; rev:8;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-BLUEDOT] Login failure from Bluedot listed IP - Specified account expired"; event_id: 532; metadata: endpoint_command FIREWALL_BLOCK_SRC_IP; classtype: unsuccessful-user; program: *Security*; bluedot: type ip_reputation, track by_src, mdate_effective_period 3 months, Malicious,Tor,Honeypot,Proxy; normalize; parse_src_ip: 1; sid:5002348; rev:8;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-BLUEDOT] Login failure from Bluedot listed IP - User not allowed to login at this computer"; event_id: 533; metadata: endpoint_command FIREWALL_BLOCK_SRC_IP; classtype: unsuccessful-user; program: *Security*; bluedot: type ip_reputation, track by_src, mdate_effective_period 3 months, Malicious,Tor,Honeypot,Proxy; normalize; parse_src_ip: 1; sid:5002349; rev:8;)
alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-BLUEDOT] Login failure from Bluedot listed IP - Account locked [0/1]"; event_id: 539|; content:!"User Name|3a| Domain|3a| Logon Type|3a|"; metadata: endpoint_command FIREWALL_BLOCK_SRC_IP; classtype: unsuccessful-user; bluedot: type ip_reputation, track by_src, mdate_effective_period 3 months, Malicious,Tor,Honeypot,Proxy; threshold: type suppress, track by_src, count 5, seconds 300; parse_src_ip: 1; parse_port; normalize; program: *Security*; sid:5002350; rev:9;)
